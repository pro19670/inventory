{
  "name": "family-inventory-backend",
  "version": 2,
  "builds": [
    {
      "src": "backend/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "USE_S3": "false",
    "USE_CHATGPT": "false"
  }
}