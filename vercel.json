{
  "version": 2,
  "name": "ai-inventory-management",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://inventory-backend-8r4j.onrender.com/api/$1"
    }
  ]
}